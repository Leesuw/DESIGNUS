<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.designus.www.dao.ImypageDao">  
     
   <select id="privacyeditSelect" parameterType="String" resultType="member">
      SELECT * FROM MB WHERE MB_ID=#{id}  
      </select>   
     
     <update id="memberreviseupdate" parameterType="member">
      UPDATE MB SET MB_PW =#{mb_pw},MB_ADDRESS=#{mb_address},MB_PROFILE=#{},MB_EMAIL=#{mb_profile} WHERE MB_ID=#{mb_id}
     </update> 
       
</mapper>